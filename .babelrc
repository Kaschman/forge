{
  "presets": [
    "@babel/preset-env",
    "@babel/flow",
    "@babel/react",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components": "./src/components"
      }
    }]
  ]
}
